@PACKAGE_INIT@

set(FoamAdapter_PROJECT_NAME "@PROJECT_NAME@")
set(FoamAdapter_PROJECT_VERSION @FoamAdapter_VERSION@)
set(FoamAdapter_PROJECT_DESCRIPTION "@FoamAdapter_DESCRIPTION@")

# Extra version information
set(FoamAdapter_VERSION_MAJOR @FoamAdapter_VERSION_MAJOR@)
set(FoamAdapter_VERSION_MINOR @FoamAdapter_VERSION_MINOR@)
set(FoamAdapter_VERSION_PATCH @FoamAdapter_VERSION_PATCH@)

# Git information
set(FoamAdapter_GIT_BRANCH "@FoamAdapter_GIT_BRANCH@")
set(FoamAdapter_GIT_REVISION "@FoamAdapter_GIT_REVISION@")
set(FoamAdapter_GIT_SHORTREV "@FoamAdapter_GIT_SHORTREV@")

# CMake build configuration
set(FoamAdapter_BUILD_TYPE "@CMAKE_BUILD_TYPE@")
set(FoamAdapter_BUILD_SHARED_LIBS "@BUILD_SHARED_LIBS@")
set(FoamAdapter_DEBUG_POSTFIX "@CMAKE_DEBUG_POSTFIX@")

# Compiler configuration
set(FoamAdapter_CXX_COMPILER "@CMAKE_CXX_COMPILER@")
set(FoamAdapter_CXX_COMPILER_VERSION @CMAKE_CXX_COMPILER_VERSION@)

set(FoamAdapter_CXX_FLAGS "@CMAKE_CXX_FLAGS@")
set(FoamAdapter_CXX_FLAGS_DEBUG "@CMAKE_CXX_FLAGS_DEBUG@")
set(FoamAdapter_CXX_FLAGS_RELEASE "@CMAKE_CXX_FLAGS_RELEASE@")

set(FoamAdapter_EXE_LINKER_FLAGS "@CMAKE_EXE_LINKER_FLAGS@")
set(FoamAdapter_EXE_LINKER_FLAGS_DEBUG "@CMAKE_EXE_LINKER_FLAGS_DEBUG@")
set(FoamAdapter_EXE_LINKER_FLAGS_RELEASE "@CMAKE_EXE_LINKER_FLAGS_RELEASE@")

# FoamAdapter configuration
set(FoamAdapter_BUILD_TESTS @FoamAdapter_BUILD_TESTS@)

set(FoamAdapter_SKIP_DEPENDENCY_UPDATE @FoamAdapter_SKIP_DEPENDENCY_UPDATE@)
set(FoamAdapter_EXPORT_BUILD_DIR @FoamAdapter_EXPORT_BUILD_DIR@)
set(FoamAdapter_VERBOSE_LEVEL @FoamAdapter_VERBOSE_LEVEL@)

set(FoamAdapter_WITH_CLANG_TIDY @FoamAdapter_WITH_CLANG_TIDY@)
set(FoamAdapter_WITH_IWYU @FoamAdapter_WITH_IWYU@)
set(FoamAdapter_CLANG_TIDY_PATH @FoamAdapter_CLANG_TIDY_PATH@)
set(FoamAdapter_IWYU_PATH @FoamAdapter_IWYU_PATH@)

# FoamAdapter compiler information
set(FoamAdapter_CXX_COMPILER "@CMAKE_CXX_COMPILER@")
set(FoamAdapter_CXX_COMPILER_SHORT "@CMAKE_CXX_COMPILER_ID@:@CMAKE_CXX_COMPILER_VERSION@")

# FoamAdapter installation configuration
set(FoamAdapter_INSTALL_PREFIX "@PACKAGE_CMAKE_INSTALL_PREFIX@")
set(FoamAdapter_INSTALL_INCLUDE_DIR "@PACKAGE_CMAKE_INSTALL_FULL_INCLUDEDIR@")
set(FoamAdapter_INSTALL_LIBRARY_DIR "@PACKAGE_CMAKE_INSTALL_FULL_LIBDIR@")
set(FoamAdapter_INSTALL_RPATH_FOR_HIP "-Wl,-rpath,${FoamAdapter_INSTALL_LIBRARY_DIR}")
set(FoamAdapter_INSTALL_RPATH @FoamAdapter_INSTALL_RPATH@)
set(FoamAdapter_INSTALL_RPATH_USE_ORIGIN @FoamAdapter_INSTALL_RPATH_USE_ORIGIN@)
set(FoamAdapter_INSTALL_RPATH_DEPENDENCIES @FoamAdapter_INSTALL_RPATH_DEPENDENCIES@)

list(APPEND CMAKE_PREFIX_PATH "${FoamAdapter_INSTALL_PREFIX}")

set(FoamAdapter_INTERFACE_LINK_LIBRARIES "@FoamAdapter_INTERFACE_LINK_LIBRARIES@")
set(FoamAdapter_INTERFACE_LINK_FLAGS "@FoamAdapter_INTERFACE_LINK_FLAGS@")
set(FoamAdapter_INTERFACE_CXX_FLAGS "@FoamAdapter_INTERFACE_CXX_FLAGS@")
