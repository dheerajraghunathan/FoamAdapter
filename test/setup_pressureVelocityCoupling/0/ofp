FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      ofp;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    fixedWalls
    {
        type            zeroGradient;
    }

    outlet
    {
        // type            zeroGradient;
        type            fixedValue;
        value           uniform 0.1;
    }

    inlet
    {
        type            zeroGradient;
    }

}


